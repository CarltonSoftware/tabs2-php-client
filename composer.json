{
    "name": "carltonsoftware/PlatoClient",
    "description": "Tabs 2 PHP Client",
    "homepage": "https://github.com/CarltonSoftware/PlatoClient",
    "license": "proprietary",
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "guzzlehttp/guzzle": "4.*",
